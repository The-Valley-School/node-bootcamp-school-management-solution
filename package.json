{
  "name": "bootcamp-school-management-solution",
  "version": "1.0.0",
  "description": "Node bootcamp",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint .",
    "start": "ts-node-dev ./src/index.ts",
    "start:pro": "node ./dist/index.js",
    "build": "tsc",
    "test:verbose": "cross-env DB_NAME=NODE-BOOTCAMP-TESTING jest",
    "test": "cross-env DB_NAME=NODE-BOOTCAMP-TESTING jest --silent",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Fran Linde",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.3",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.2.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
